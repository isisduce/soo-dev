{
    "cors": {
        "enabled": true,
        "allowedMethods": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
        "allowedHeaders": [ "Content-Type", "Authorization" ],
        "credentials": true,
        "origin": [
            "http://localhost:8080",
            "http://localhost:8890",
            "http://192.168.0.101:8080",
            "http://192.168.0.101:8890",
            "http://isisduce.iptime.org:8080"
        ]
    },
    "webFilesystem": {
        "rootPath": "/nas",
        "uploadPath": "/nas/apps/web-filesystem/upload",
        "allowedPaths": [
            { "name": "Root", "path": "/nas" },
            { "name": "Upload", "path": "/nas/apps/web-filesystem/upload" },
            { "name": "CCC", "path": "C:/" },
            { "name": "DDD", "path": "D:/" },
            { "name": "EEE", "path": "E:/" },
            { "name": "FFF", "path": "F:/" }
        ],
        "maxDepth": 15,
        "enableHiddenFiles": false,
        "showJunctionPoints": true,
        "showSymbolicLinks": true,
        "hideInaccessibleLinks": true
    }
}